{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app.js"
    }
  ],
  "crons": [
    {
      "path": "/api/v1/cron",
      "schedule": "35 16 * * *"  // Run at 10:05 PM IST (4:35 PM UTC)
    }
  ]
}
